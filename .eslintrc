{
  "extends": "airbnb",
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "class-methods-use-this": 0,
    "max-len": [
      1,
      {
        "code": 200
      }
    ],
    "consistent-return": 0,
    "func-names": 0,
    "no-underscore-dangle": [
      0
    ]
  }
}